class World

operations

-- run a simulation
public run: () ==> ()
run() == (
	start(TorsionBar`ctrl);
	start(TorsionBar`user);
	block();
);

-- wait for simulation to finish
block: () ==> ()
block() == skip;
sync per block => true;

values

-- SDPs
public ENC_COUNTS: real = 0;
public USE_SCRIPT: real = 0;
public FAIL: real = 0;

end World